{
  "name": "RRS test",
  "title": "RRS test",
  "inputs": [
    {
      "name": "bam",
      "class": "file",
      "patterns": [
        "*.bam"
      ],
      "help": "Aligned transcriptome data"
    },
    {
      "name": "bai",
      "class": "file",
      "patterns": [
        "*.bai"
      ],
      "help": "Index file for aligned transcriptome data"
     },
     {
      "name": "genome",
      "class": "string",
      "choices": ["GRCh37-lite", "GRCh38_no_alt"],
      "default": "GRCh37-lite"
     }
  ],
  "outputs": [
    {
      "name": "coverage_bw",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "coverage_bw",
          "stage": "coverage"
        }
      }
    },
    {
      "name": "fusions",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "fusions",
          "stage": "fuzzion"
        }
      }
    },
    {
      "name": "junctions",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "junctions",
          "stage": "rnapeg"
        }
      }
    },
    {
      "name": "junctions_shifted",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "junctions_shifted",
          "stage": "rnapeg"
        }
      }
    },
    {
      "name": "junctions_shifted_bed",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "junctions_shifted_bed",
          "stage": "rnapeg"
        }
      }
    },
    {
      "name": "junctions_annotated",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "junctions_annotated",
          "stage": "rnapeg"
        }
      }
    },
    {
      "name": "cicero_output",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "cicero_output",
          "stage": "cicero"
        }
      }
    },
    {
      "name": "cicero_final_fusions",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "cicero_final_fusions",
          "stage": "cicero"
        }
      }
    },
    {
      "name": "cicero_final_fusions_html",
      "class": "file",
      "outputSource": {
        "$dnanexus_link": {
          "outputField": "cicero_final_fusions_html",
          "stage": "cicero"
        }
      }
    }
  ],
  "stages": [
    {
      "id": "coverage",
      "executable": "app-stjude_coverage_bw/1.2.0",
      "input": {
        "BAM": {
          "$dnanexus_link": {
            "workflowInputField": "bam"
          }
        },
        "BAM_INDEX": {
          "$dnanexus_link": {
            "workflowInputField": "bai"
          }
        },
        "ref_name": {
          "$dnanexus_link": {
            "workflowInputField": "genome"
          }
        }
      }
    },
    {
      "id": "fuzzion",
      "executable": "app-stjude_fuzzion/1.1.0",
      "input": {
        "BAM": {
          "$dnanexus_link": {
            "workflowInputField": "bam"
          }
        }
      }
    },
    {
      "id": "rnapeg",
      "executable": "app-stjude_rnapeg/1.3.0",
      "input": {
        "bam": {
          "$dnanexus_link": {
            "workflowInputField": "bam"
          }
        },
        "bam_index": {
          "$dnanexus_link": {
            "workflowInputField": "bai"
          }
        },
        "ref_name": {
          "$dnanexus_link": {
            "workflowInputField": "genome"
          }
        }
      }
    },
    {
      "id": "cicero",
      "executable": "app-stjude_cicero/1.7.1",
      "input": {
        "star_mapped_sorted_bam": {
          "$dnanexus_link": {
            "workflowInputField": "bam"
          }
        },
        "bam_index": {
          "$dnanexus_link": {
            "workflowInputField": "bai"
          }
        },
        "rnapeg_junctions": {
          "$dnanexus_link": {
            "stage": "rnapeg",
            "outputField": "junctions_shifted"
          }
        },
        "ref_name": {
          "$dnanexus_link": {
            "workflowInputField": "genome"
          }
        }
      }
    },
    {
      "id": "visualize_coverage",
      "executable": "app-stjude_build_visualizer/1.1.1",
      "input": {
        "viewer_file": {
          "$dnanexus_link": {
            "project": "project-F5444K89PZxXjBqVJ3Pp79B4",
            "id": "file-F6YJf5Q93pQbpk5Y6kYqkfkf"
          }
        },
        "files_to_view": [
          {
            "$dnanexus_link": {
              "stage": "coverage",
              "outputField": "coverage_bw"
            }
          }
        ],
        "output_name": "Coverage"
      }
    },
    {
      "id": "visualize_fusions",
      "executable": "app-stjude_build_visualizer/1.1.1",
      "input": {
        "viewer_file": {
          "$dnanexus_link": {
            "project": "project-F5444K89PZxXjBqVJ3Pp79B4",
            "id": "file-F6YJf6j93pQjy7vF7X3b7p7z"
          }
        },
        "files_to_view": [
          {
            "$dnanexus_link": {
              "stage": "cicero",
              "outputField": "cicero_final_fusions"
            }
          }
        ],
        "output_name": "Putative Fusions"
      }
    },
    {
      "id": "tag_outputs",
      "executable": "app-stjude_tag_outputs/1.0.0",
      "input": {
        "files_to_tag": [
          {
            "$dnanexus_link": {
              "stage": "coverage",
              "outputField": "coverage_bw"
            }
          },
          {
            "$dnanexus_link": {
              "stage": "fuzzion",
              "outputField": "fusions"
            }
          },
          {
            "$dnanexus_link": {
              "stage": "rnapeg",
              "outputField": "junctions"
            }
          },
          {
            "$dnanexus_link": {
              "stage": "cicero",
              "outputField": "cicero_output"
            }
          },
          {
            "$dnanexus_link": {
              "stage": "cicero",
              "outputField": "cicero_final_fusions"
            }
          },
          {
            "$dnanexus_link": {
              "stage": "cicero",
              "outputField": "cicero_final_fusions_html"
            }
          }
        ]
      }
    }
  ]
}
